import numpy as np
def norm(A, B):
    """
    Takes two Numpy column arrays, A and B, and returns the L2 norm of their
    sum.

    Arg:
      A - a Numpy array
      B - a Numpy array
    Returns:
      s - the L2 norm of A+B.
    """
    s=np.linalg.norm(A+B)
    return s
 
x=np.array([1,2,3,4,5,6])
x
y=np.array([7,8,9,10,11,12])
y
print(norm(x,y))